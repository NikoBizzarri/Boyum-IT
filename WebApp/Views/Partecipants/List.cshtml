@model WebApp.Models.PartecipantListViewModel

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>Fullname</th>
            <th>Email</th>
            <th>Phone</th>
            <th>EventId</th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.Partecipant) {
        <tr>
            <td>
                @item.Fullname
            </td>
            <td>
                @item.Email
            </td>
            <td>
                @item.Phone
            </td>
            <td>
                @item.EventId
            </td>
        </tr>
}
    </tbody>
</table>
